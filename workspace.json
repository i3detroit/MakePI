{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "make-pi",
  "projects": {
    "make-pi": {
      "root": "apps/make-pi",
      "sourceRoot": "apps/make-pi/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/make-pi",
            "main": "apps/make-pi/src/main.ts",
            "tsConfig": "apps/make-pi/tsconfig.app.json",
            "assets": [
              "apps/make-pi/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/make-pi/src/environments/environment.ts",
                  "with": "apps/make-pi/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "make-pi:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/make-pi/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/make-pi"
          ],
          "options": {
            "jestConfig": "apps/make-pi/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-database": {
      "root": "libs/shared/database",
      "sourceRoot": "libs/shared/database/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/shared/database/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/shared/database"
          ],
          "options": {
            "jestConfig": "libs/shared/database/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}