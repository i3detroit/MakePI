{
  "version": 2,
  "projects": {
    "auth": {
      "root": "libs/shared/auth",
      "sourceRoot": "libs/shared/auth/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/auth/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/libs/shared/auth"],
          "options": {
            "jestConfig": "libs/shared/auth/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "bcrypt": {
      "root": "libs/shared/bcrypt",
      "sourceRoot": "libs/shared/bcrypt/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/bcrypt/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/libs/shared/bcrypt"],
          "options": {
            "jestConfig": "libs/shared/bcrypt/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "database": {
      "root": "libs/shared/database",
      "sourceRoot": "libs/shared/database/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/database/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/libs/shared/database"],
          "options": {
            "jestConfig": "libs/shared/database/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "make-pi": {
      "root": "apps/make-pi",
      "sourceRoot": "apps/make-pi/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/webpack:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/make-pi",
            "main": "apps/make-pi/src/main.ts",
            "tsConfig": "apps/make-pi/tsconfig.app.json",
            "assets": ["apps/make-pi/src/assets"],
            "target": "node",
            "compiler": "tsc"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/make-pi/src/environments/environment.ts",
                  "with": "apps/make-pi/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "buildTarget": "make-pi:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/make-pi/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/apps/make-pi"],
          "options": {
            "jestConfig": "apps/make-pi/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "models-users": {
      "root": "libs/models/users",
      "sourceRoot": "libs/models/users/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/models/users/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/libs/models/users"],
          "options": {
            "jestConfig": "libs/models/users/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    },
    "roles": {
      "root": "libs/roles",
      "sourceRoot": "libs/roles/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/roles/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["{workspaceRoot}/coverage/libs/roles"],
          "options": {
            "jestConfig": "libs/roles/jest.config.ts",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "$schema": "./node_modules/nx/schemas/workspace-schema.json"
}
